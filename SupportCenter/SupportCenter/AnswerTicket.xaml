<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SupportCenter.AnswerTicket">
  <StackLayout Orientation="Vertical" Padding="10">
  <Label Text="Response"/>
  <Entry x:Name="Response" />
<Label Text="Client?"/>
  <Switch x:Name="Client" IsEnabled="False"/>
    <Button Text="Create Ticket!" Clicked="Button_OnClicked"/>
    </StackLayout>
</ContentPage>